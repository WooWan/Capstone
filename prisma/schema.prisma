generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id     String  @id @default(cuid())
  name   String?
  musics Music[]
}

model Music {
  id         String  @id @default(cuid())
  title      String
  artist     String
  albumCover String
  musicUrl   String
  bassUrl    String?
  guitarUrl  String?
  drumUrl    String?
  pianoUrl   String?
  vocalUrl   String?
  otherUrl   String?
  userId     String  @map("user_id")
  user       User    @relation(fields: [userId], references: [id], onDelete: Cascade)
}
